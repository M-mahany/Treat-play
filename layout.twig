<!doctype html>
<html class="no-js" lang='en' dir='ltr'>

<head>
  <meta charset="utf-8">
  <meta http-equiv="x-ua-compatible" content="ie=edge">

  {{ header_meta_tags | raw }}


  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">

  <link rel="shortcut icon" type="image/jpg" href={{ store.icon }} />

  {#
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Changa"> #}
  <!--jquery UI -->
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link
    href="https://fonts.googleapis.com/css2?family={% if settings.fonts_name %}{{ settings.fonts_name }}{% else %}Changa{% endif %}:wght@300;400;500;700&display=swap"
    rel="stylesheet">

  {% if session.lang.code == 'ar' %}
  <link rel="stylesheet" type="text/css" href="{{ asset_url ~ 'bootstrap.min.css?v=1.01' }}" />
  {% else %}
  <link rel="stylesheet" type="text/css" href="{{ asset_url ~ 'bootstrap-rtl.css?v=1.05' }}" />

  {% endif %}

  <link rel="stylesheet" type="text/css" href="{{ asset_url ~ 'bootstrap-fixes.css?v=1.11' }}" />


  <link rel="stylesheet" type="text/css" href="{{ asset_url ~ 'jquery-ui.min.css' }}" />
  <link rel="stylesheet" type="text/css" href="{{ asset_url ~ 'jquery-ui.structure.min.css' }}" />
  <link rel="stylesheet" type="text/css" href="{{ asset_url ~ 'jquery-ui.theme.min.css' }}" />

  <link rel="stylesheet" type="text/css" href="{{ asset_url ~ 'slick.css' }}" />
  <link rel="stylesheet" type="text/css" href="{{ asset_url ~ 'slick-theme.css?v=1.07' }}" />

  <link rel="stylesheet" type="text/css" href="{{ asset_url ~ 'slide-menu.css' }}" />

  <link rel="stylesheet" type="text/css" href="{{ asset_url ~ 'slide-menu-style.css?v=1.16' }}" />

  <link rel="stylesheet" href="{{ asset_url ~ 'main.css?v=1.219' }}" />

  <link rel="stylesheet" type="text/css" href="{{ asset_url ~ 'custom.css?v=1.146' }}" />

  {# {% if session.lang.code == 'ar' %}
  <link rel="stylesheet" href="{{ asset_url ~ 'main.rtl.css?v=1.25' }}">
  {% endif %} #}

  {% block top_links %}{% endblock %}

  {% if user_custom_css %}
  <link rel="stylesheet" href="{{ user_custom_css }}">
  {% endif %}

</head>



<body class="ltr" data-template="{{ template }}" {% if template=='category' and category is not empty %}
  data-category-id="{{ category.id }}" {% endif %}>
  {% if store.editing_mode %}
  <link
    href="https://fonts.googleapis.com/css2?family={% if settings.fonts_name %}{{ settings.fonts_name }}{% else %}Changa{% endif %}:wght@300;400;500;700&display=swap"
    rel="stylesheet">
  {% endif %}

  <style>
    body,
    html {
      font-family: {
        % if settings.fonts_name %
      }

        {
          {
          strReplace('+', ' ', settings.fonts_name)
        }
      }

        {
        % else %
      }

      'Changa' {
        % endif %
      }

      ,
      sans-serif;
    }

    :root {
        {
        % if settings.colors_header_background_color %
      }

      --header-primary-color: {
          {
          settings.colors_header_background_color
        }
      }

      ;

        {
        % endif %
      }

        {
        % if settings.colors_header_menu_background_color %
      }

      --menu-header-primary-color: {
          {
          settings.colors_header_menu_background_color
        }
      }

      ;
      --header-primary-brightness: brightness(100%);

        {
        % elseif settings.colors_header_background_color %
      }

      --menu-header-primary-color: {
          {
          settings.colors_header_background_color
        }
      }

      ;
      --header-primary-brightness: brightness(80%);

        {
        % endif %
      }

        {
        % if settings.colors_header_menu_text_color %
      }

      --menu-header-text-color-primary-bg: {
          {
          settings.colors_header_menu_text_color
        }
      }

      ;

        {
        % elseif settings.colors_header_text_color %
      }

      --menu-header-text-color-primary-bg: {
          {
          settings.colors_header_text_color
        }
      }

      ;

        {
        % endif %
      }

        {
        % if settings.colors_header_text_color %
      }

      --header-text-color-primary-bg: {
          {
          settings.colors_header_text_color
        }
      }

      ;

        {
        % endif %
      }

        {
        % if settings.colors_footer_background_color %
      }

      --footer-primary-color: {
          {
          settings.colors_footer_background_color
        }
      }

      ;

        {
        % endif %
      }

        {
        % if settings.colors_footer_text_color %
      }

      --footer-text-color-primary-bg: {
          {
          settings.colors_footer_text_color
        }
      }

      ;

        {
        % endif %
      }
    }
  </style>

  {% block top_body %}{% endblock %}

  <div class="app d-flex flex-column visible">
    <div class="header">
      {% block header %}{% endblock %}
    </div>
    <div class="main flex-grow-1 app-content" {% if settings.colors_background_image %}
      style="background-repeat: repeat; background-image: url('{{ settings.colors_background_image }}');" {% endif %}>
      {% block main_content %}{% endblock %}
    </div>
    <div class="footer">
      {% block footer %}{% endblock %}
    </div>
    {% block footer_sticky %}{% endblock %}
  </div>

  {{ zidapi_script|raw }}

  <script>
    //window.appDirection = '{% if session.lang.code == 'ar' %}rtl{% else %}ltr{% endif %}';

    window.loadToasterScriptIfNotLoaded = function (onCompleted) {
      var list = document.getElementsByTagName('script');
      var i = list.length, flag = false;
      while (i--) {
        if (list[i].src === "{{ asset_url ~ 'toastr.min.js' }}") {
          flag = true;
          break;
        }
      }

      if (!flag) {
        var script = document.createElement('script');
        script.src = "{{ asset_url ~ 'toastr.min.js' }}";
        document.getElementsByTagName('head')[0].appendChild(script);

        var fileref = document.createElement("link");
        fileref.rel = "stylesheet";
        fileref.type = "text/css";
        fileref.href = "{{ asset_url ~ 'toastr.min.css' }}";
        document.getElementsByTagName("head")[0].appendChild(fileref)

        script.onload = function () {
          if (onCompleted)
            onCompleted();
        }
      } else {
        if (onCompleted)
          onCompleted();
      }
    }
  </script>

  <script src="{{ asset_url ~ 'jquery-3.6.0.min.js?v=1.0.0' }}"></script>

  <script src="{{ asset_url ~ 'jquery-ui.min.js' }}"></script>

  <script src="{{ asset_url ~ 'popper.min.js?v=1.0' }}"></script>
  <script src="{{ asset_url ~ 'bootstrap.min.js' }}"></script>

  <script src="{{ asset_url ~ 'slide-menu.ie.js?v=1.06' }}"></script>
  <script src="{{ asset_url ~ 'slick.min.js' }}"></script>

  <script type="text/javascript" src="{{ asset_url ~ 'main.js?v=1.117' }}"></script>

  {% block footer_scripts %}{% endblock %}

</body>

</html>


